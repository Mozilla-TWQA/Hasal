{
  "module-name"             : "FrameThroughputDctGenerator",
  "module-path"             : "lib.generator.frameThroughputDctGenerator",
  "sample-converter-name"   : "FTSampleConverter",
  "sample-converter-path"   : "lib.converter.ftSampleConverter",
  "image-converter-name"    : "Cv2Converter",
  "image-converter-path"    : "lib.converter.cv2Converter",
  "image-converter-format"  : "bmp",
  "case-type"               : "re",
  "ipynb-template-name"     : "report-frame-throughput.ipynb",
  "snapshot-base-sample1"   : true,
  "snapshot-base-sample2"   : false,
  "supported-suite-fp"      : "",
  "drop-outlier-flag"       : false,
  "upload-perfherder-data-field": "med_time",
  "region"                  : {
    "start": {"event": "start", "search_target": "start_region", "direction": "backward"},
    "end": {"event": "end", "search_target": "end_region", "direction": "forward"}
  },
  "platform-dep-settings"   : {
    "darwin":{
      "default": {
        "video-recording-fps": 90,
        "compare-threshold": 0.01,
        "search-margin": 10,
        "mismatch-rate-threshold": 0
      }
    },
    "win32":{
      "default": {
        "video-recording-fps": 90,
        "compare-threshold": 0.01,
        "search-margin": 10,
        "mismatch-rate-threshold": 0
      }
    },
    "linux2":{
      "default": {
        "video-recording-fps": 90,
        "compare-threshold": 0.01,
        "search-margin": 10,
        "mismatch-rate-threshold": 0
      }
    }
  }
}
